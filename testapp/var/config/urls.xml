<?xml version="1.0" encoding="iso-8859-1"?>
<urls xmlns="http://jelix.org/ns/urls/1.0">
    <classicentrypoint name="index" default="true">
        <url pathinfo="/test/:annee/:mois" module="testapp" action="test1">
              <param name="annee" escape="true" type="year"/>
              <!--<param name="mois" escape="false" />-->
              <static name="bla" value="cequejeveux" />
        </url>

        <url pathinfo="/test/news/:annee/:mois/:id" module="jelix_tests" action="urlsig_url1" actionoverride="urlsig_url8,urlsig_url9,urlsig_url10">
           <param name="annee" regexp="\d{4}"/>
           <param name="mois"  regexp="\d{2}" />
           <param name="id"   regexp="\d{2}" />
        </url>

        <url pathinfo="/test/cms/:rubrique/:id_art-:article" module="jelix_tests" action="urlsig_url3">
           <param name="rubrique" />
           <param name="id_art" regexp="\d+"/>
           <param name="article" escape="true" type="string"/>
        </url>

        <url pathinfo="/test/cms2/:rubrique/:id_art" module="jelix_tests" action="urlsig_url6" noentrypoint="true">
           <param name="rubrique" />
           <param name="id_art" regexp="\d+"/>
        </url>

        <url pathinfo="/shop/:rubrique/:id_article" module="jelix_tests" https="true" action="urlsig_url11">
           <param name="rubrique" />
           <param name="id_article" type="int"/>
        </url>
        <url pathinfo="/shop/:rubrique/:id_article/" module="jelix_tests" action="urlsig_url12">
           <param name="rubrique" />
           <param name="id_article" type="int"/>
        </url>
        <url pathinfo="/supershop/:rubrique" module="jelix_tests" action="urlsig_url13" optionalTrailingSlash="true">
           <param name="rubrique" />
        </url>
    </classicentrypoint>
    <classicentrypoint name="testnews">
       <url pathinfo="/:annee/:mois" module="jelix_tests" action="urlsig_url2">
          <param name="annee" type="year"/>
          <param name="mois"  type="mounth" />
          <static name="mystatic" value="valeur statique" />
        </url>
    </classicentrypoint>
    <classicentrypoint name="foo/bar">
       <url handler="urlsig" module="jelix_tests" action="urlsig_url4" />
    </classicentrypoint>

    <classicentrypoint name="news">
        <url module="news" />
    </classicentrypoint>
    <xmlrpcentrypoint name="xmlrpc" default="true" />
    <jsonrpcentrypoint name="jsonrpc" default="true" />
</urls>